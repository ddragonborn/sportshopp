<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="540.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sport.AdminScreenController">
   <children>
      <ImageView fitHeight="145.0" fitWidth="145.0" layoutX="113.0" layoutY="81.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../img/profile 1.png" />
         </image>
      </ImageView>
      <Label fx:id="roleField" layoutX="300.0" layoutY="81.0" prefHeight="36.0" prefWidth="202.0" text="Role">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label fx:id="fioField" layoutX="300.0" layoutY="137.0" prefHeight="26.0" prefWidth="353.0" text="Fio">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="phoneField" layoutX="300.0" layoutY="191.0" text="phone">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button fx:id="btnOrders" layoutX="110.0" layoutY="281.0" mnemonicParsing="false" onAction="#goOrders" prefHeight="26.0" prefWidth="150.0" style="-fx-background-color: #333542;" text="Заказы" textFill="WHITE">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnHistory" layoutX="111.0" layoutY="323.0" mnemonicParsing="false" onAction="#goHistory" prefHeight="39.0" prefWidth="150.0" style="-fx-background-color: #333542;" text="История входа" textFill="WHITE">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="110.0" layoutY="423.0" mnemonicParsing="false" onAction="#goExit" prefHeight="39.0" prefWidth="150.0" style="-fx-background-color: #333542;" text="Выход" textFill="WHITE">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="otcet" layoutX="735.0" layoutY="423.0" mnemonicParsing="false" onAction="#clot" prefHeight="55.0" prefWidth="178.0" style="-fx-background-color: #333542;" text="Сформировать отчёт" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="dobavTov" layoutX="110.0" layoutY="373.0" mnemonicParsing="false" onAction="#tov" prefHeight="36.0" prefWidth="150.0" style="-fx-background-color: #333542;" text="Добавить товар" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
